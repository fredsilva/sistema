<?xml version="1.0" encoding="UTF-8"?>
<!-- Stylus Studio -->
<!-- Created september 2016 by Santiago Reyes Benalcazar (CIAT) -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xsd:element name="sdeDocument">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Definição de esquema documento para o sistema Tocantins SAT</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="sdeDocumentHeader"/>
				<xsd:element ref="sdeDocumentFields"/>
				<xsd:element ref="sdeParagraphs" minOccurs="0"/>
				<xsd:element ref="sdeDocumentPages" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="sdeDocumentHeader">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Contém informações gerais sobre o documento como informações autor , informações de conta corrente, entre outros</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="globalDocId"/>
				<xsd:element ref="formCode"/>
				<xsd:element ref="formVersion"/>
				<xsd:element ref="globalStatus"/>
				<xsd:element ref="generalInfo"/>
				<xsd:element ref="currentAccountInfo"/>
				<xsd:element ref="authorInfo"/>
				<!-- eliminar
				<xsd:element ref="reporterInfo" minOccurs="0"/>
				-->
			</xsd:all>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="sdeDocumentFields">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Uma lista de todos os campos que definem um documento</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="field" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="field">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Especificar as informações e os atributos de um campo específico no documento</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:sequence>
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element ref="radioOption"/>
					<xsd:element ref="sdeTable"/>
					<xsd:element ref="checkBoxOption"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute ref="colspan"/>
			<xsd:attribute ref="calculate"/>
			<xsd:attribute ref="required"/>
			<xsd:attribute ref="class"/>
			<xsd:attribute ref="page" use="required"/>
			<xsd:attribute ref="id" use="required"/>
			<xsd:attribute ref="fieldType"/>
			<xsd:attribute ref="row"/>
			<xsd:attribute ref="text-align"/>
			<xsd:attribute ref="evaluate"/>
			<xsd:attribute ref="col"/>
			<xsd:attribute ref="dataType"/>
			<!-- eliminar
			<xsd:attribute ref="dbColumn"/>
			-->
			<xsd:attribute ref="radioLayout"/>
			<xsd:attribute ref="calcorder"/>
			<xsd:attribute ref="readonly"/>
			<xsd:attribute ref="money"/>
			<xsd:attribute ref="requiredmessage"/>
			<xsd:attribute ref="validation"/>
			<xsd:attribute ref="validationmessage"/>
			<xsd:attribute ref="searchable"/>
			<xsd:attribute ref="listName"/>
			<xsd:attribute ref="longName"/>
			<!--
			<xsd:attribute ref="tPartyMap"/>
			-->
			<xsd:attribute ref="dbFunction"/>
			<xsd:attribute ref="onFieldUpdatedId"/>
			<xsd:attribute ref="triggerFieldUpdateFunction"/>
			<xsd:attribute ref="returnParameterOrder"/>
			<xsd:attribute ref="persistableColumn"/>
			<!-- eliminar
			<xsd:attribute ref="fieldNumber"/>
			-->
			<xsd:attribute ref="generalFunctionMap"/>
			<!-- eliminar
			<xsd:attribute ref="documentType"/>
			-->
			<!-- eliminar 
			<xsd:attribute ref="triggerFieldUpdateEvent"/>
			-->
			<!-- eliminar 
			<xsd:attribute ref="hiddenAction"/>
			-->
			<xsd:attribute ref="subDocumentFieldId"/>
			<!--
			<xsd:attribute ref="showOnline"/>
			 eliminar
			<xsd:attribute ref="tpIndividualFieldIdMap"/>
			-->
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="formCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Identifica cada formulário com um código único</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="19"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="formVersion" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Ele permite controlar versões das formas</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="docTypeId" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de documento identificador anexado</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="globalStatus" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">
O status do documento, especialmente usado para diferenciar se um documento está no projecto de estatuto , é que foi salvo, mas fórmulas e cálculos ainda não foram validados aplicada ; estado é enviada, que já está validações aplicadas e ações foram definidas aqui corridas foram executados após o recebimento no sistema ( é possível que afetou outros subsistemas, como conta corrente, BPMs , etc.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="obligationCode" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Código de	obrigação tributaria que afeta este documento</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:attribute name="class" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Converte diretamente para um atributo de estilo CSS; O atributo deve ser definido nos documentos de folhas de estilo</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="readonly" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">O campo será somente leitura</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="money" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Se o campo é um valor monetário</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<!-- eliminar
	<xsd:attribute name="tpIndividualFieldIdMap">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">T ODO</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="30"/>
				<xsd:pattern value="([A-Za-z]([A-Za-z0-9_]+)?)"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
-->
	<!-- eliminar
	<xsd:attribute name="type" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">T ODO</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
-->
	<xsd:attribute name="colspan" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Número de colunas que o campo se estende para dentro da matriz de 16 colunas;</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="calculate" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Contiente a fórmula de cálculo em javascript formato o resultado será exibido no campo;</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="required" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Se for necessário inserir um valor no campo;</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="page" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Documento página em que o campo está localizado , por padrão, apenas a primeira página, se houver um;</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="id">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Campo identificador exclusivo</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="30"/>
				<xsd:pattern value="([A-Za-z]([A-Za-z0-9_]+)?)"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="fieldType" type="fieldType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Identifica como o campo é exibido no formulário para capturar; Lista de valores predefinidos digite : FieldType</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="row" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Linha na qual o campo será exibido</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="text-align" type="textAlign">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Definir o alinhamento (esquerda, centro, direita )</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="evaluate" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Se o campo deve ser avaliada durante a validação</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="col" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Coluna no qual exibir o campo</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="dataType" type="dmsDataType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Tipo de dados que suporta o campo</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="radioLayout" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Se o " rádio" tipo de campo é exibido horizontalmente ou verticalmente</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="calcorder" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Um número inteiro em relação ao resto dos campos que determinam em que ordem fórmulas serão calculados</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="requiredmessage" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Mensagem para exibir se o campo é obrigatório e necessário para especificar um mensaja mais descritivo que " Campo obrigatório" que seria mostrado se não houver nenhum padrão este valor;</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="validation" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Formula "JavaScript" type para validar o valor inserido no campo; Se a fórmula não retorna verdadeiro valor , mensagem de validação será exibido e não ser autorizados a apresentar o formulário</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="validationmessage" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Mensagem a ser exibida quando a validação de campo não for atendida</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="order" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">A ordem em que os valores do tipo de lista de campos " radioOption " ou " checkBoxOption " será exibido</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="colnum" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Em casos de campos multiregistro , a ordem na qual a coluna é exibida no formulário</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="width" type="unit">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">En los casos de los campos multiregistro, el ancho de la columna a desplegar en porcentaje</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="title" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Em casos de campos multiregistro , o título da coluna a ser exibido</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="totalRowFunction" type="totalRowFunction">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Em casos de multiregistro campos que indica se esta coluna terá um total calculado , usando o " javascript" soma função ou avg</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<!-- eliminar
	<xsd:attribute name="dbColumn" type="dbColumn">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">T ODO</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	-->
	<xsd:attribute name="totalRowLabel" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Para os casos de campos multiregistro , a etiqueta terá o total no final de uma coluna que tenha valor calculado</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<!-- eliminar
	<xsd:attribute name="fieldNumber" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation>To identify fields by cell number for batch key purpouses</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	-->

	<xsd:attribute name="numberOfRows" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Para campos do tipo " multiregistro " o número máximo de linhas a serem exibidas na forma</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>

	<!-- eliminar
	<xsd:attribute name="totalRow" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">T ODO</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	-->
	<xsd:attribute name="listName" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">O valor deve pertencer a uma lista de catálogos do sistema SAT , que será acessível ao gerar a forma de captura; Se esse valor existe nessa lista, o formulário irá mostrar os valores desse catálogo em particular , através de um rótulo "HTML Select"</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="searchable" type="yesNo">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Se o valor for verdadeiro , este campo deve ser indexado para acelerar as pesquisas ea tela de busca entrar neste campo para que o usuário digite o valor quando você selecionar este tipo de formulário</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<xsd:attribute name="longName">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Nome do campo longa</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="30"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<!-- eliminar
	<xsd:attribute name="documentType">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">T ODO</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="ASSESSMENT"/>
