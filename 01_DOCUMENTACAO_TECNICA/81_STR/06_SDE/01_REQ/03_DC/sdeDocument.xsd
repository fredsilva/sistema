	<xsd:element name="sdeDocumentPages">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">Seção para descrever as páginas de um formulário; Eles usaram mais de uma página para separar logicamente os campos e facilitar os dados de implantação e dedilhado; Se mais de uma página de cada campo deve especificar qual página será exibida</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="sdePage" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="sdePage">
		<xsd:annotation>
			<xsd:documentation xml:lang="pt">especifica o nome eo número de uma página no formulário; deve ter pelo menos um</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="page" type="xsd:integer" use="required"/>
			<xsd:attribute name="selected" type="xsd:boolean"/>
			<xsd:attribute name="title" use="required"/>
		</xsd:complexType>
	</xsd:element>
