				<xsd:enumeration value="OBJECTION_TO_REASSESSMENT"/>
				<xsd:enumeration value="DECISION_TO_OBJECTION_OF_ASSESSMENT"/>
				<xsd:enumeration value="DECISION_TO_OBJECTION_OF_REASSESSMENT"/>
			</xsd:restriction>
