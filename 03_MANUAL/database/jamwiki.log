INSERT INTO JAM_TOPIC_VERSION VALUES(777,347,'','Acessar a tela atrav\u00e9s do ''''''Menu de Usu\u00e1rio > Alterar senha. ''''''\u000a\u000aA tela tamb\u00e9m \u00e9 acessada quando o usu\u00e1rio acessa o sistema pela primeira vez.\u000a\u000aS\u00f3 podem alterar senha aqueles usu\u00e1rios que n\u00e3o acessaram com certificado digital. \u000a\u000a== Alterando a Senha de Acesso ==\u000aO sistema permite a altera\u00e7\u00e3o da senha de acesso atrav\u00e9s do Menu de Usu\u00e1rio > Alterar senha, ou quando o usu\u00e1rio acessa pela primeira vez o sistema, ele \u00e9 direcionado automaticamente para a tela, conforme ilustrado no passo 1 abaixo.\u000a\u000a[[File:alterarSenha.jpg|Alterar Senha]]\u000a\u000aPara gravar a nova senha o usu\u00e1rio deve preencher todos os campos da tela e clicar em Salvar (passo 1).\u000a\u000aPara cancelar a altera\u00e7\u00e3o da senha de acesso, deve-se clicar no bot\u00e3o Fechar (Passo 2), ou clicar fora da janela de altera\u00e7\u00e3o.\u000a',1,'192.168.0.197','2016-07-18 17:50:30.426000',1,733,-42,NULL)
COMMIT
