<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib version="2.2"
            xmlns="http://xmlns.jcp.org/xml/ns/javaee"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">

    <namespace>http://sefaz.to.gov.br/composites</namespace>
    <composite-library-name>composites</composite-library-name>

    <tag>
        <tag-name>panel</tag-name>
        <source>../resources/composites/panel.xhtml</source>

        <attribute>
            <name>id</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>title</name>
            <type>java.lang.String</type>
        </attribute>
    </tag>

    <tag>
        <tag-name>saveUpdateButton</tag-name>
        <source>../resources/composites/saveUpdateButton.xhtml</source>

        <attribute>
            <name>save</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>update</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>label</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>render</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>execute</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>onEvent</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>crudMb</name>
            <type>br.gov.to.sefaz.presentation.managedbean.CrudMB</type>
        </attribute>
    </tag>

    <tag>
        <tag-name>opcaoAjudaButton</tag-name>
        <source>../resources/composites/opcaoAjudaButton.xhtml</source>

        <attribute>
            <name>label</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <type>java.lang.String</type>
        </attribute>
    </tag>
</facelet-taglib>